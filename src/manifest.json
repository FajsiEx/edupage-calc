{
    "name": "EduPage Tools",
    "version": "1.1",
    "description": "Rewritten in Angularrrrr",
    "manifest_version": 2,
    "icons": {
        "16": "assets/icon16.png",
        "32": "assets/icon32.png",
        "48": "assets/icon48.png",
        "128": "assets/icon128.png"
    },
    "content_scripts": [
        {
            "matches": [
                "*://*.edupage.org/*"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "browser_action": {
        "default_popup": "index.html"
    }
}